{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "logger": {
            "type": "object",
            "properties": {
                "level": {
                    "type": "string",
                    "enum": ["trace", "debug", "info", "warn", "error"]
                }
            },
            "required": ["level"]
        }
    },
    "required": ["logger"]
}
